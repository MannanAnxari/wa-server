{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node",
        "config": {
          "buildCommand": "npm install"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js",
        "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
      }
    ],
    "env": {
      "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD": "false"
    }
  }
  
  