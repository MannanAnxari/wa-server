{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        },
        {
            "src": "index.js",
            "use": "@vercel/node",
            "config": {
                "buildCommand": "chmod +x vercel-build.sh && ./vercel-build.sh"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.js",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "PATCH",
                "DELETE",
                "OPTIONS"
            ]
        }
    ],
    "env": {
        "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD": "true",
        "CHROME_PATH": "/usr/bin/google-chrome-stable"
    }
}